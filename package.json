{
  "name": "oudit",
  "version": "0.1.0",
  "type": "module",
  "description": "Vue.js + TypeScript project managed with Nix",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src --ext .vue,.ts,.js",
