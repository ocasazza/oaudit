{
  "name": "oudit",
  "version": "0.1.0",
  "homepage": "https://ocasazza.github.io/oaudit/",
  "type": "module",
  "description": "Nuxt.js + TypeScript project managed with Nix",
  "scripts": {
    "dev": "nuxt dev",
    "build": "rm -rf ./dist && nuxt build",
    "generate": "rm -rf ./dist && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "type-check": "nuxt typecheck",
    "lint": "eslint . --ext .vue,.ts,.js",
    "lint:fix": "eslint . --ext .vue,.ts,.js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^6.12.1",
    "nuxt": "^3.13.0",
    "tailwindcss-themer": "^4.1.1",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.5.7",
    "@nuxt/typescript-build": "^3.0.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "vue-tsc": "^1.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
